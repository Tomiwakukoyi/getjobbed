{
  "expo": {
    "name": "Get Jobbed",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",

    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.the.tommyk.GetJobbed"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.the.tommyk.GetJobbed"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "555bbfaa-f0f4-411f-aa4f-8e706ad69f9b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/555bbfaa-f0f4-411f-aa4f-8e706ad69f9b"
    }
  }
}
